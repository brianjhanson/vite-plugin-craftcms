{
  "name": "vite-plugin-craftcms",
  "version": "3.0.0",
  "description": "Use Vite with Craft CMS. No Craft plugin necessary",
  "files": [
    "dist"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "unbuild",
    "prepublishOnly": "npm run build",
    "release": "np"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "contributors": [
    {
      "name": "Brian Hanson",
