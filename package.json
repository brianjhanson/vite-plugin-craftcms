      "url": "https://www.brianhanson.net/"
    },
    {
      "name": "Chris Malven",
      "url": "https://malven.co/"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brianjhanson/vite-plugin-craftcms",
    "directory": "src"
  },
  "bugs": {
    "url": "https://github.com/brianjhanson/vite-plugin-craftcms/issues"
  },
  "homepage": "https://github.com/brianjhanson/vite-plugin-craftcms#readme",
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.3",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "np": "^8.0.4",
    "prettier": "^3.1.0",
    "rollup": "^4.5.0",
    "typescript": "^5.3.2",
    "unbuild": "^2.0.0",
    "vite": "^5.0.2",
    "vite-plugin-inspect": "^0.7.42",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "node-html-parser": "^6.1.11"
  }
}
